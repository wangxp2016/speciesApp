<!doctype html>
<html class="no-js" lang="en">

<head>
    <% include meta.ejs %>
</head>

<body>


    <!--[if lt IE 8]>
	<p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
	<![endif]-->

    <div class="wrapper">
        <% include header.ejs %>
        <!--Breadcrumb One Start-->
        <div class="breadcrumb-one mb-120">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="breadcrumb-img">
                            <img src="/layout/img/page-banner/product-banner.jpg" alt="">
                        </div>
                        <div class="breadcrumb-content">
                            <ul>
                                <li><a href="/">Home</a></li>
                                <li class="active">Single Species</li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="product-description-review-area mb-100">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="product-review-tab">
                            <!--Review And Description Tab Menu Start-->
                            <ul class="nav dec-and-review-menu">
                                <li>
                                    <a class="active" data-toggle="tab" href="#description">物种信息</a>
                                </li>
                            </ul>
                            <label class="col-md-2 control-label">
                                order:
                            </label>
                            <span>
                                <%=speciesDetail.order_m%></span>
                            <br>
                            <label class="col-md-2 control-label">
                                suborder:
                            </label>
                            <span>
                                <%=speciesDetail.suborder%></span>
                            <br>
                            <label class="col-md-2 control-label">
                                superfamily:
                            </label>
                            <span>
                                <%=speciesDetail.superfamily%></span>
                            <br>
                            <label class="col-md-2 control-label">
                                family:
                            </label>
                            <span>
                                <%=speciesDetail.family%></span>
                            <br>
                            <label class="col-md-2 control-label">
                                subfamily:
                            </label>
                            <span>
                                <%=speciesDetail.subfamily%></span>
                            <br>
                            <label class="col-md-2 control-label">
                                genus:
                            </label>
                            <span>
                                <%=speciesDetail.genus%></span>
                            <br>
                            <label class="col-md-2 control-label">
                                species:
                            </label>
                            <span>
                                <%=speciesDetail.species%></span>
                            <br>
                            <label class="col-md-2 control-label">
                                introduction:
                            </label>
                            <span>
                                <%=speciesDetail.introduction%></span>
                            <br>
                            <label class="col-md-2 control-label">
                                地理分布图:
                            </label>
                            <div class="tab-content single-product-img">
                                <% let distributionpics = speciesDetail.distribution_img.split(',') %>
                                <% for(let i=0,len=distributionpics.length;i<len;i++){ %>
                                <div class="tab-pane fade show active" id="product<%=i%>" style="width: 300px;height: 200px;float: left">
                                    <div class="product-large-thumb img-full">
                                        <div class="easyzoom easyzoom--overlay">
                                            <a href="<%=distributionpics[i]%>">
                                                <img style="width: 300px;height: 225px" src="<%=distributionpics[i]%>"
                                                    alt="">
                                            </a>
                                            <a href="<%=distributionpics[i]%>" class="popup-img venobox" data-gall="myGallery"><i
                                                    class="fa fa-search"></i></a>
                                        </div>
                                    </div>
                                </div>
                                <% } %>
                            </div>
                            <div style="clear:both;"></div>
                            <br>
                            <label class="col-md-2 control-label">
                                地理分布简介:
                            </label>
                            <span>
                                <%=speciesDetail.distribution%></span>
                            <br>
                            <label class="col-md-2 control-label">
                                形态图:
                            </label>
                            <div class="tab-content single-product-img">
                                <% let morphologypics = speciesDetail.morphology_img.split(',') %>
                                <% for(let i=0,len=morphologypics.length;i<len;i++){ %>
                                <div class="tab-pane fade show active" id="product<%=i%>" style="width: 300px;height: 200px;float: left">
                                    <div class="product-large-thumb img-full">
                                        <div class="easyzoom easyzoom--overlay">
                                            <a href="<%=morphologypics[i]%>">
                                                <img style="width: 300px;height: 225px" src="<%=morphologypics[i]%>"
                                                    alt="">
                                            </a>
                                            <a href="<%=morphologypics[i]%>" class="popup-img venobox" data-gall="myGallery"><i
                                                    class="fa fa-search"></i></a>
                                        </div>
                                    </div>
                                </div>
                                <% } %>
                            </div>
                            <div style="clear:both;"></div>
                            <br>
                            <label class="col-md-2 control-label">
                                性别:
                            </label>
                            <span>
                                <%=speciesDetail.sex%></span>
                            <br>
                            <label class="col-md-2 control-label">
                                体色:
                            </label>
                            <span>
                                <%=speciesDetail.body_color%></span>
                            <br>
                            <label class="col-md-2 control-label">
                                体长:
                            </label>
                            <span>
                                <%=speciesDetail.body_length%></span>
                            <br>
                            <label class="col-md-2 control-label">
                                前胸背板长:
                            </label>
                            <span>
                                <%=speciesDetail.pronotum_length%></span>
                            <br>
                            <label class="col-md-2 control-label">
                                前翅长:
                            </label>
                            <span>
                                <%=speciesDetail.forewing_length%></span>
                            <br>
                            <label class="col-md-2 control-label">
                                后足股节长:
                            </label>
                            <span>
                                <%=speciesDetail.hindFemur_length%></span>
                            <br>
                            <label class="col-md-2 control-label">
                                参看文献:
                            </label>
                            <span>
                                <%=speciesDetail.literature%></span>
                            <br>
                            <label class="col-md-2 control-label">
                                备注:
                            </label>
                            <span>
                                <%=speciesDetail.remark%></span>
                            <br>
                            <label class="col-md-2 control-label">
                                附件下载:
                            </label>
                            <div id="main" style="min-height: 10px">
                                <ul id="browser" class="filetree">
                                    <li><span class="folder">
                                            <%=speciesDetail.species%></span>
                                        <ul>
                                            <% let fileList = speciesDetail.fileList %>
                                            <% for(let i=0,len=fileList.length;i<len;i++){ %>
                                            <li class="closed"><span class="folder">
                                                    <%=fileList[i].name%></span>
                                                <ul id=<%=fileList[i].name%>>
                                                    <% let speciesFile = fileList[i].children %>
                                                    <% for(let j=0,len=speciesFile.length;j<len;j++){ %>
                                                    <li> <img src="layout/img/icon/Package.png" style="width: 20px;height: 20px" alt="">
                                                        <a href="<%=speciesFile[j].value.replace('www','')%>"><%=speciesFile[j].name%></a></li>
                                                    <%}%>
                                                </ul>
                                            </li>
                                            <%}%>
                                        </ul>
                                    </li>
                                </ul>
                            </div>
                            <!-- file download end-->
                            <!--Review And Description Tab Content Start-->
                            <div class="tab-content product-review-content-tab" id="myTabContent-4">
                                <div class="tab-pane fade active show" id="description">
                                    <div class="single-product-description">
                                        <%-speciesDetail.species_summary%>
                                    </div>
                                </div>
                            </div>
                            <!--Review And Description Tab Content End-->
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <% include footer.ejs %>
        <!-- Modal Area Strat -->
        <div class="modal fade" id="open-modal" tabindex="-1" role="dialog" aria-hidden="true">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><i class="fa fa-close"></i></button>
                    </div>
                    <div class="modal-body">
                        <div class="row">
                            <!--Modal Img-->
                            <div class="col-md-5">
                                <!--Modal Tab Content Start-->
                                <div class="tab-content product-details-large" id="myTabContent">
                                    <div class="tab-pane fade show active" id="single-slide1" role="tabpanel"
                                        aria-labelledby="single-slide-tab-1">
                                        <!--Single Product Image Start-->
                                        <div class="single-product-img img-full">
                                            <img src="/layout/img/single-product/large/single-product1.jpg" alt="">
                                        </div>
                                        <!--Single Product Image End-->
                                    </div>
                                    <div class="tab-pane fade" id="single-slide2" role="tabpanel" aria-labelledby="single-slide-tab-2">
                                        <!--Single Product Image Start-->
                                        <div class="single-product-img img-full">
                                            <img src="/layout/img/single-product/large/single-product2.jpg" alt="">
                                        </div>
                                        <!--Single Product Image End-->
                                    </div>
                                    <div class="tab-pane fade" id="single-slide3" role="tabpanel" aria-labelledby="single-slide-tab-3">
                                        <!--Single Product Image Start-->
                                        <div class="single-product-img img-full">
                                            <img src="/layout/img/single-product/large/single-product3.jpg" alt="">
                                        </div>
                                        <!--Single Product Image End-->
                                    </div>
                                    <div class="tab-pane fade" id="single-slide4" role="tabpanel" aria-labelledby="single-slide-tab-4">
                                        <!--Single Product Image Start-->
                                        <div class="single-product-img img-full">
                                            <img src="/layout/img/single-product/large/single-product4.jpg" alt="">
                                        </div>
                                        <!--Single Product Image End-->
                                    </div>
                                    <div class="tab-pane fade" id="single-slide5" role="tabpanel" aria-labelledby="single-slide-tab-4">
                                        <!--Single Product Image Start-->
                                        <div class="single-product-img img-full">
                                            <img src="/layout/img/single-product/large/single-product5.jpg" alt="">
                                        </div>
                                        <!--Single Product Image End-->
                                    </div>
                                    <div class="tab-pane fade" id="single-slide6" role="tabpanel" aria-labelledby="single-slide-tab-4">
                                        <!--Single Product Image Start-->
                                        <div class="single-product-img img-full">
                                            <img src="/layout/img/single-product/large/single-product6.jpg" alt="">
                                        </div>
                                        <!--Single Product Image End-->
                                    </div>
                                </div>
                                <!--Modal Content End-->
                                <!--Modal Tab Menu Start-->
                                <div class="single-product-menu">
                                    <div class="nav single-slide-menu owl-carousel" role="tablist">
                                        <div class="single-tab-menu img-full">
                                            <a class="active" data-toggle="tab" id="single-slide-tab-1" href="#single-slide1"><img
                                                    src="/layout/img/single-product/small/single-product1.jpg" alt=""></a>
                                        </div>
                                        <div class="single-tab-menu img-full">
                                            <a data-toggle="tab" id="single-slide-tab-2" href="#single-slide2"><img src="/layout/img/single-product/small/single-product2.jpg"
                                                    alt=""></a>
                                        </div>
                                        <div class="single-tab-menu img-full">
                                            <a data-toggle="tab" id="single-slide-tab-3" href="#single-slide3"><img src="/layout/img/single-product/small/single-product3.jpg"
                                                    alt=""></a>
                                        </div>
                                        <div class="single-tab-menu img-full">
                                            <a data-toggle="tab" id="single-slide-tab-4" href="#single-slide4"><img src="/layout/img/single-product/small/single-product4.jpg"
                                                    alt=""></a>
                                        </div>
                                        <div class="single-tab-menu img-full">
                                            <a data-toggle="tab" id="single-slide-tab-5" href="#single-slide5"><img src="/layout/img/single-product/small/single-product5.jpg"
                                                    alt=""></a>
                                        </div>
                                        <div class="single-tab-menu img-full">
                                            <a data-toggle="tab" id="single-slide-tab-6" href="#single-slide6"><img src="/layout/img/single-product/small/single-product6.jpg"
                                                    alt=""></a>
                                        </div>
                                    </div>
                                </div>
                                <!--Modal Tab Menu End-->
                            </div>
                            <!--Modal Img-->
                            <!--Modal Content-->
                            <div class="col-md-7">
                                <div class="modal-product-info">
                                    <h1>Sit voluptatem</h1>
                                    <div class="modal-product-price">
                                        <span class="old-price">$74.00</span>
                                        <span class="new-price">$69.00</span>
                                    </div>
                                    <a href="single-product.ejs" class="see-all">See all features</a>
                                    <div class="add-to-cart quantity">
                                        <form class="add-quantity" action="#">
                                            <div class="modal-quantity">
                                                <input type="number" value="1">
                                            </div>
                                            <div class="add-to-link">
                                                <button class="form-button" data-text="add to cart">add to cart</button>
                                            </div>
                                        </form>
                                    </div>
                                    <div class="cart-description">
                                        <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod
                                            tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim
                                            veniam, quis nostrud exercitation ullamco,Proin lectus ipsum, gravida et
                                            mattis vulputate, tristique ut lectus.</p>
                                    </div>
                                    <div class="social-share">
                                        <h3>Share this product</h3>
                                        <ul class="socil-icon2">
                                            <li><a href=""><i class="fa fa-facebook"></i></a></li>
                                            <li><a href=""><i class="fa fa-twitter"></i></a></li>
                                            <li><a href=""><i class="fa fa-pinterest"></i></a></li>
                                            <li><a href=""><i class="fa fa-google-plus"></i></a></li>
                                            <li><a href=""><i class="fa fa-linkedin"></i></a></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                            <!--Modal Content-->
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Modal Area End -->

        <!-- 联系我们 -->
        <div class="modal fade companyModel" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel"
            data-backdrop="static" data-keyboard="false">
            <div class="modal-dialog modal-sm" role="document">
                <div class="modal-content">
                    <div class="modal-header"></div>
                    <div class="modal-body">
                        <div class="tab-content">
                            <div class="tab-pane active">
                                <style>
                                    .list-unstyled p {
                                        display: inline-block;
                                        margin-left: 10px;
                                    }

                                    .list-unstyled a {
                                        color: #a43d21
                                    }
                                </style>
                                <ul class="list-unstyled activity-timeline">
                                    <li>
                                        <i class="lnr lnr-apartment activity-icon"></i>
                                        <p class="AddressId"></p>
                                    </li>
                                    <li>
                                        <i class="lnr lnr-phone activity-icon"></i>
                                        <a href="" class="PhoneIdLink">
                                            <p class="PhoneId"></p>
                                        </a>
                                    </li>
                                    <li>
                                        <i class="lnr lnr-link activity-icon"></i>
                                        <p class="EmailId"></p>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button class="btn btn-xs btn-primary" data-dismiss="modal">关闭</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <% include script.ejs %>
    <script type="text/javascript">
        $(function () {
            $("#browser").treeview();
            $("#browser").bind("contextmenu", function (event) {
                if ($(event.target).is("li") || $(event.target).parents("li").length) {
                    $("#browser").treeview({
                        remove: $(event.target).parents("li").filter(":first")
                    });
                    return false;
                }
            });
        })
    </script>
</body>

</html>