                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Modal Area End -->
    </div>
    <% include script.ejs %>
    <script src="/layout/js/jqpaginator.min.js"></script>
    <style>
        .first a,
        .prev a,
        .next a,
        .last a {
            width: 64px !important;
        }

        .page-header {
            padding-bottom: 9px;
            margin: 40px 0 20px;
            border-bottom: 1px solid #eeeeee;
        }

        .tab-content a {
            color: #ef6c00;
            text-decoration: none;
        }

        .product-pagination>ul {
            text-align: center;
            padding-top: 30px;
            margin-bottom: 40px;
            /*border-top: 1px solid #e5e5e5;*/
        }

        .actives a {
            cursor: text
        }

        .actives a:hover {
            color: #ffffff !important;
        }
    </style>
    <script>
        var word = '',
            hrefId = '';
        location.href.search('word') > -1 ? word = location.href.split('word')[1].split('=')[1] : word = '';

        // 设置跳转地址
        hrefId = '/overview';

        // 分页及参数设置
        try {
            $('#jqpaginatorId').jqPaginator({
                totalPages: parseInt('<%=speciesList.totalPage%>'),
                totalCounts: parseInt('<%=speciesList.totalRows%>'),
                visiblePages: 5,
                currentPage: parseInt('<%=speciesList.page%>'),
                activeClass: 'actives',
                disableClass: 'actives',
                first: '<li class="first"><a href="/overview?page=1&word=' + word + '">首页</a></li>',
                prev: '<li class="prev""><a  href="/overview?page={{page}}&word=' + word + '">上一页</a></li>',
                next: '<li class="next"><a  href="/overview?page={{page}}&word=' + word + '">下一页</a></li>',
                last: '<li class="last"><a  href="/overview?page={{totalPages}}&word=' + word + '">尾页</a></li>',
                page: '<li class="page"><a  href="/overview?page={{page}}&word=' + word + '">{{page}}</a></li>',
                onPageChange: function (num) {
                    // $('#text').html('当前第' + num + '页');
                    $('.actives a').removeAttr('href');
                }
            });
        } catch (e) {
            $('#info').css({
                'display': 'block'
            })
        }
    </script>
</body>

</html>