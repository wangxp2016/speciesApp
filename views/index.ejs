	<% include script.ejs %>
	<script>
		$(function () {
			var msg = $('#mc-email'),
				tip = $('#alertModel'),
				btn = $('#mc-submit');
			btn.click(function () {
				if (msg.val()) {
					$.ajax({
						url: '/common/consult',
						type: 'POST',
						data: {
							phone: msg.val()
						},
						dataType: 'JSON',
						beforeSend: function () {
							btn.attr({
								disabled: 'disabled'
							});
						},
						success: function (res) {
							tip.text(res.msg);
							tip.css({
								'display': 'block'
							});
							setTimeout(function () {
								tip.css({
									'display': 'none'
								});
							}, 3000)
						},
						complete: function () {
							btn.removeAttr('disabled');
							msg.val('');
						}
					})
				}
			});
		})
	</script>
